{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug servidor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/dserver",
            "args": [ "-a", "a", "s", "1", "a"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "make",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
            
        }
    ]
}
